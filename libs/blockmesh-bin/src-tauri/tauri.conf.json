{
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "blockmesh"
  },
  "tauri": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://cloudflare-worker-ip-data.blockmesh.workers.dev/{{target}}/{{arch}}/{{current_version}}"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDgwNzZDOUI3ODlDNDE5NTAKUldSUUdjU0p0OGwyZ0oyS3B1YmJyeCtXb0ZOVktlU2RFdnpGc0V6VUdDcVFvQWN3cmNGditZQ20K",
      "windows": {
        "installMode": "passive"
      }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "blockmesh",
        "width": 1050,
        "height": 700,
        "minWidth": 1050,
        "minHeight": 700
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "xyz.blockmesh",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
